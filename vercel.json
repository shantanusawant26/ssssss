{
  "version": 2,
  "builds": [
    { "src": "backend/app.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build" ,
    "config": { "distDir": "docs" }}
  ],
  "routes": [
    { "src": "/contact/(.*)", "dest": "/backend/app.js" },
    { "src": "/(.*)", "dest": "dd/index.html" }
  ]
}
